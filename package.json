{
	"name": "req-real-ip",
	"version": "1.0.4",
	"description": "This library allows you to get the real ip address of a user. It gives you the real ip address even if the request header is modified.",
	"main": "dist/index.js",
	"scripts": {
		"dev": "tsx --watch src/index.ts",
		"build": "rm -rf dist && tsup src/index.ts --format esm",
		"build:watch": "rm -rf dist && tsup src/index.ts --watch --format esm"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"real",
		"ip",
		"address",
		"request",
		"header",
		"modified",
		"x-forwarded-for",
		"x-real-ip",
		"x-client-ip",
		"x-proxy-user",
		"x-cluster-client-ip",
		"forwarded",
		"user-ip",
		"express-ip",
		"express-real-ip",
		"express-request-ip",
		"express-real-ip-address",
		"express-real-ip-address-middleware",
		"express-real-ip-address-middleware-nodejs",
		"express-real-ip-address-middleware-node",
		"express-real-ip-address-middleware-js"
	],
	"author": "zfcsoftware",
	"maintainers": [
		{
			"name": "LacheRo"
		}
	],
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lacherogwu/req-real-ip"
	},
	"dependencies": {
		"request-ip": "^3.3.0"
	},
	"devDependencies": {
		"@types/node": "^22.7.7",
		"@types/request-ip": "^0.0.41",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3"
	}
}
